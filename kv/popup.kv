# confirmar
<Confirmar>
    title: 'CONFIRMAÇÃO'
    bold: True
    title_align: 'center'
    size: (1200, 1000)
    size_hint: (None, None)
    auto_dismiss: True
    separator_color: 0.91, 0.94, 0.56, 1
    FloatLayout:
        Label:
            pos_hint: {"right": 1, "top": 1}
            size_hint: 1, 0.85
            valign: 'middle'
            bold: True
            text: "     DESEJA CONFIRMAR\n       TODOS OS DADOS\n         REGISTRADOS?"
            font_size: dp(24)
        Button:
            canvas.before:
                Color:
                    rgba: (0.44, 0.93, 0.48, 1)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [20]
            background_color: (0, 0, 0, 0)
            pos_hint: {"right": 0.5, "top": 0.2}
            size_hint: 0.5, 0.15
            font_name: "righteous"
            text: "SIM"
            bold: True
            font_size: dp(20)
            color: 0, 0, 0, 1
            on_release: app.mudar_tela('escolharotapage')
            on_press: root.dismiss()
        Button:
            canvas.before:
                Color:
                    rgba: (0.92, 0.48, 0.44, 1)
                RoundedRectangle:
                    size: self.size
                    pos: self.pos
                    radius: [20]
            background_color: (0, 0, 0, 0)
            pos_hint: {"right": 1, "top": 0.2}
            size_hint: 0.5, 0.15
            font_name: "righteous"
            text: 'NÃO'
            font_size: dp(20)
            color: 0, 0, 0, 1
            bold: True
            on_press: root.dismiss()
